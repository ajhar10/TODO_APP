<% for(var i=0;i<users.length;i++){%>
<tbody>
    <tr>
        <td><%=i+1%></td>
        <td><%=users[i].name%></td>
        <td><%=users[i].email %></td>
        <td><%=users[i].gender%></td>
        <td><%= users[i].status%></td>
        <td>
            <a href="/update-user" class="btn border-shadow update"><span class="text-gradient"><i class="fa-solid fa-pencil"></i></span></a>
            <a class="btn border-shadow delete"><span class="text-gradient"><i class="fa-sharp fa-solid fa-xmark"></i></span></a>
        </td>
    </tr>
</tbody>
<%}%>